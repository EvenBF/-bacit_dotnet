@model bacit_dotnet.MVC.Models.SuggestionModel
<h1>
   Forslagsliste
</h1>

<ul>
    @foreach (var user in Model.Sug)
    {
        <li>
            <div>
            <h3> @user.Title</h3>
            <p>Navn: @user.Name</p> 
            <p>Team: @user.Team </p> 
            <p>Beskrivelse: @user.Description</p>
            <button type="button">Rediger</button>
            </div>
        </li>
    }
</ul>